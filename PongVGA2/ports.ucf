#Clock Pin
NET "CLK_IN" LOC = "C9" | IOSTANDARD = LVCMOS33 ;

#SWTICHES
NET "BUTTONS<0>" LOC = "L14" | IOSTANDARD = LVTTL | PULLUP ;
NET "BUTTONS<1>" LOC = "L13" | IOSTANDARD = LVTTL | PULLUP ;
NET "NORTH" LOC = "V4" | IOSTANDARD = LVTTL | PULLDOWN ;

