#For casado's VGA Ports
NET "R<2>"      LOC = "D5" | IOSTANDARD = LVTTL | DRIVE = 8 | SLEW = FAST ;
NET "R<1>"      LOC = "A4" | IOSTANDARD = LVTTL | DRIVE = 8 | SLEW = FAST ;
NET "R<0>"      LOC = "B4" | IOSTANDARD = LVTTL | DRIVE = 8 | SLEW = FAST ;

NET "G<2>"      LOC = "C5" | IOSTANDARD = LVTTL | DRIVE = 8 | SLEW = FAST ;
NET "G<1>"      LOC = "B6" | IOSTANDARD = LVTTL | DRIVE = 8 | SLEW = FAST ;
NET "G<0>"      LOC = "A6" | IOSTANDARD = LVTTL | DRIVE = 8 | SLEW = FAST ;

