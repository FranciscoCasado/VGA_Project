NET "B<0>"      LOC = "F8" | IOSTANDARD = LVTTL | DRIVE = 8 | SLEW = FAST ;

NET "H_SYNC" LOC = "D7" | IOSTANDARD = LVTTL | DRIVE = 8 | SLEW = FAST ;
